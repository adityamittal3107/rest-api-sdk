{
    "$schema": "https://titan.apimatic.io/api/build/schema",
    "buildFileVersion": "1.0",
    "generatePortal": {
        "pageTitle": "ThoughtSpot Rest API playground v2.0",
        "navTitle": "Public Rest APIs",
        "logoUrl": "static/images/logo.png",
        "logoLink": "https://www.thoughtspot.com",
        "languageConfig": {
            "http": {}
        },
        "headIncludes": "<link href=\"https:\\/\\/fonts.googleapis.com\\/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i&display=swap\" rel=\"stylesheet\">",
        "tailIncludes": "<script id=\"ts-playground-snippet\" src=\".\/static\/js\/embedded.js\"><\/<script>",
        "portalSettings": {
            "themeOverrides": {
                "themeType": "cool",
                "palette": {
                    "primaryColor": "#2770EF",
                    "linkColor": "#2770EF",
                    "colors": {
                        "C100": "#F9FAFE",
                        "C000": "#FFFFFF",
                        "C200": "#F6F8FA",
                        "C300": "#C0C6CF",
                        "C400": "#CED2E2",
                        "C500": "#B0B5C9",
                        "C600": "#1D232F",
                        "C700": "#606E8A",
                        "C800": "#1D232F",
                        "C900": "#1C2029",
                        "C1000": "#1D2236"
                    }
                },
                "fontSource": [],
                "cssStyles": {
                    "headings": {
                        "fontFamily": "Optimo-Plain"
                    },
                    "body": {
                        "fontFamily": "Optimo-Plain"
                    },
                    "code": {
                        "fontFamily": "Optimo-Plain",
                        "blockCode": {
                            "fontFamily": "Optimo-Plain",
                            "fontSize": "12px",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "lineHeight": "19.5px"
                        },
                        "inlineCode": {
                            "fontFamily": "Optimo-Plain",
                            "fontSize": "12px",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "lineHeight": "19.5px"
                        }
                    }
                }
            },
            "enableExport": false,
            "enableConsoleCalls": true
        }
    }
}